{
  "name": "StopMotionCollaborator",
  "version": "1.0.0",
  "description": "A web application for collaborative stop-motion video production optimized for mobile landscape orientation",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:github-pages": "vite build --base=/koma-mobile/",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rimraf dist",
    "predeploy": "npm run build:github-pages"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.830.0",
    "jszip": "^3.10.1",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.830.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/jszip": "^3.4.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "express": "^5.1.0",
    "happy-dom": "^18.0.1",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
